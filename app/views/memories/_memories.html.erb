<% memories.each do |memory| %>
  <div class="row">
    <div class="col s12 m6">
      <div class="card">
        <div class="card-image">
          <div class="memory_image">
            <%= link_to memory_path(memory.id), method: :GET do %>
              <img src= "<%= memory.image.url %>" >
            <% end %>
            </div>
        </div>
      </div>
    </div>
  </div>
<% end %>
