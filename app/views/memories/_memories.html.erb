<% memories.each do |memory| %>
  <div class="row">
    <div class="col s12 m6">
      <div class="card">
        <div class="card-image">
          <div class="memory_image"><img src= "<%= memory.image.url %>" ></div>
          <span class="card-title"><%= memory.country%>
          <p class="cretated-user__name"><%= memory.user.name %></p>
          </span>
        </div>
        <i class="material-icons">chat</i>
        <%= link_to new_memory_path, method: :GET %>
        <div class="likes-count">
          <span class="likes-count">いいね! <%= memory.likes_count %>件</span>
        </div>
      </div>
    </div>
  </div>
<% end %>
